  "AutoSkillWhen": "begins battle with $2 if applicable",
  "RegenerateN": "$1% $2 each turn",
  "InvigorateN": "$1 $2 each turn",
  "GrowthN": "$1% $2 gained when not active",
  "LifeAidN": "$1% max $2 after battle",
  "Recarm": "$1% $2 after reviving",
  "Endure": "$2 with 1 HP after a mortal blow once per battle",
