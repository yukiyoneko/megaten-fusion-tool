  "ElemCharge": "dmg dealt by next $2 attack x$1",
  "ElemWall": "adds $2 resistance for 3 turns",
  "ElemBreak": "removes $2 resistance for 3 turns",
  "ElemBlock": "nullifies next $2 attack",
  "ElemKarn": "repels next $2 attack",
  "ElemMaster": "$2 skill costs x$1",
  "ElemBoost": "$2 dmg dealt x$1",
  "HealBoost": "$2 skill pwr x$1",
  "DodgeElem": "$2 evasion x$1",
  "ElemPierce": "$2 dmg dealt cannot be resisted, nullified, or drained",
  "ResistElem": "$2 dmg taken x$1",
  "NullElem": "nullifies $2 dmg taken",
  "RepelElem": "repels $2 dmg taken",
  "DrainElem": "drains $2 dmg taken",
