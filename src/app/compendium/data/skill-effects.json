{
  "Base": "$2",
  "Exact": "$1% $2",
  "Plus": "$2 +$1%",
  "Times": "$2 x$1",
  "PowerAgainst": "$1 pwr against $2",
  "FracDamage": "reduces $2 by $1%",
  "InstakillWhen": "$1% instakill when $2",
  "SmtKaja": "raises $2 1 rank",
  "SmtKunda": "lowers $2 1 rank",
